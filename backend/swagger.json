{
	"openapi":"3.0.0",
	"info":{
		"title":"Parking Up API",
		"description":"Essa API foi desenvolvida para o primeiro desafio do StationOne e tem como propósito oferecer ferramentas de gestão para um sistema de estacionamento de veículos.",
		"contact":{
			"email":"barbaramabrito@gmail.com"
		},
		"version":"1.0.0"
	},
	"servers":[
		{
			"url":"http://localhost:5000",
			"description":"API de desenvolvimento/teste"
		},
		{
			"url":"https://parkingupapi.onrender.com",
			"description":"API de produção"
		}
	],
	"paths":{
		"/usuarios/register":{
			"post":{
				"summary":"Cadastro de usuários",
				"description":"Rota para cadastro de usuários",
				"tags":["Usuario"],
				"requestBody":{
					"content":{
						"application/json":{
							"schema":{
								"$ref":"#/components/schemas/Usuario"
							}
						}
					}
				},
				"responses":{
					"201":{
						"description":"Created",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			}
		},
		"/usuarios":{
			"get":{
				"summary":"Lista todos os usuários",
				"description":"Rota para listagem de todos os usuários cadastrados",
				"tags":["Usuario"],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			}
		},
		"/usuarios/{id}":{
			"get":{
				"summary":"Lista um usuário pelo id",
				"description":"Rota para listagem de um usuário pelo id",
				"tags":["Usuario"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"id do usuário",
						"required":true
					}
				],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			},
			"put":{
				"summary":"Atualiza um usuário",
				"description":"Rota para atualização do usuario",
				"tags":["Usuario"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"Atualiza o usuário pelo id",
						"required":true
					}
				],
				"requestBody":{
					"content":{
						"application/json":{
							"schema":{
								"$ref":"#/components/schemas/Usuario"
							}
						}
					}
				},
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			},
			"delete":{
				"summary":"Remove um usuário",
				"description":"Deleta um usuário pelo id",
				"tags":["Usuario"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"Id do usuário",
						"required":true
					}
				],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			}
		},
