	"components":{
		"schemas":{
			"Usuario":{
				"type":"object",
				"properties":{
					"nome":{
						"type":"string"
					},
					"email":{
						"type":"string"	
					},
					"cpf":{
						"type":"string"
					},
					"dataNascimento":{
						"type":"string"
					}
				}
			},
			"Veiculo":{
				"type":"object",
				"properties":{
					"marca":{
						"type":"string"
					},
					"modelo":{
						"type":"string"
					},
					"ano":{
						"type":"string"
					},
					"placaVeiculo":{
						"type":"string"
					}
				}
			}
		}
	}
}