			"post":{
				"summary":"Cadastra um veículo de um usuário",
				"description":"Rota para cadastro de veículos de um determinado usuário",
				"tags":["Usuario"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"Id do usuário",
						"required":true
					}
				],
				"requestBody":{
					"content":{
						"application/json":{
							"schema":{
								"$ref":"#/components/schemas/Veiculo"
							}
						}
					}
				},
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			},
			"get":{
				"summary":"Retorna um veículo do usuário",
				"description":"Retorna um veículo do usuário pelo id",
				"tags":["Usuario"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"id do usuário",
						"required":true
					}
				],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Usuario"
								}
							}
						}
					}
				}
			}
		},
		"/veiculos":{
			"get":{
				"summary":"Lista todos os veículos",
				"description":"Rota para a listagem de todos os veículos existentes",
				"tags":["Veiculo"],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Veiculo"
								}
							}
						}
					}
				}
			}
		},
		"/veiculos/{id}":{
			"get":{
				"summary":"Lista um veículo por id",
				"description":"Rota que que retorna um veículo pelo id",
				"tags":["Veiculo"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"id do veículo",
						"required":true
					}
				],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Veiculo"
								}
							}
						}
					}
				}
			},
			"put":{
				"summary":"Atualiza um veículo",
				"description":"Rota para atualizar um veículo",
				"tags":["Veiculo"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"Atualiza o veículo pelo id",
						"required":true
					}
				],
				"requestBody":{
					"content":{
						"application/json":{
							"schema":{
								"$ref":"#/components/schemas/Veiculo"
							}
						}
					}
				},
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Veiculo"
								}
							}
						}
					}
				}
			},
			"delete":{
				"summary":"Remove um veículo",
				"description":"Rota para deletar um veículo pelo id",
				"tags":["Veiculo"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"Id do veículo",
						"required":true
					}
				],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Veiculo"
								}
							}
						}
					}
				}
			}
		},
		"/veiculos/{id}/usuario":{
			"get":{
				"summary":"Retorna o dono de um veículo",
				"description":"Rota que retorna o dono de um determinado veículo",
				"tags":["Veiculo"],
				"parameters":[
					{
						"name":"id",
						"in":"path",
						"description":"id do veículo",
						"required":true
					}
				],
				"responses":{
					"200":{
						"description":"OK",
						"content":{
							"application/json":{
								"schema":{
									"type":"object",
									"$ref":"#/components/schemas/Veiculo"
								}
							}
						}
					}
				}
			}
		}
